{
  "manifest_version": 2,
  "version": "0.1.1",
  "name": "Mute Tab",
  "description": "Full control of noisy tabs with keyboard shortcut support",
  "permissions": [
    "storage"
   ],
   "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "homepage_url": "https://add0n.com/mute-tab.html",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png"
  },
  "browser_action":{
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    },
    "default_popup": "data/popup/index.html"
  },
  "commands": {
    "mute-tab": {
      "description": "Mute Tab"
    },
    "mute-other-tabs-window": {
      "description": "Mute Other Tabs in This Window"
    },
    "mute-tabs-other-windows": {
      "description": "Mute Tabs in Other Windows"
    },
    "mute-all-other-tabs": {
      "description": "Mute All Other Tabs"
    },
    "unmute-tab": {
      "description": "UnMute Tab"
    },
    "unmute-other-tabs-window": {
      "description": "UnMute Other Tabs in This Window"
    },
    "unmute-tabs-other-windows": {
      "description": "UnMute Tabs in Other Windows"
    },
    "unmute-all-other-tabs": {
      "description": "UnMute All Other Tabs"
    }
  }
}
